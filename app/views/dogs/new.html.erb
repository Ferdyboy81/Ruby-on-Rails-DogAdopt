<h1>New Dogs Available for Adoption</h1>


<%= form_for(@dog, url: '/dogs/index') do |f| %>
  <%= f.label :breed %>
  <%= f.text_field :breed %><br />

  <%= f.label :age %>
  <%= f.text_field :age %><br />

  <%= f.label :sex %>
  <%= f.text_field :sex %><br />

  <%= f.label :image %>
  <%= f.text_field :image %><br />

  <%= f.submit "submit" %>
<% end %>

 <%= link_to 'Back', index_dog_path %>

<h1>New Dogs Available for Adoption</h1>

<!-- <p>Find me in app/views/dogs/new.html.erb</p>
 -->

<%= form_for @dog do |f| %>
  <%= f.label :breed %>
  <%= f.text_field :breed %><br />

  <%= f.label :age %>
  <%= f.text_field :age %><br />

  <%= f.label :sex %>
  <%= f.text_field :sex %><br />

  <%= f.submit %>
<% end %>

 <%= link_to 'Back', dogs_path %>

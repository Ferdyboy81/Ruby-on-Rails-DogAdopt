<h1>Edit your Dog profile</h1>
<!-- <p>Find me in app/views/dogs/edit.html.erb</p> -->

<%= form_for @dog do |f| %>

  <%= f.label :breed %>
  <%= f.text_field :breed %><br />

  <%= f.label :age %>
  <%= f.text_field :age %><br />

  <%= f.label :sex %>
  <%= f.text_field :sex %><br />

  <%= f.label :image %>
  <%= f.text_field :image %><br />

  <%= f.submit "Save changes", class: "btn btn-primary" %>
<% end %>

 <%= link_to 'Back', dog_path %>
<%= link_to 'Delete', delete_path, :method => :delete %>
